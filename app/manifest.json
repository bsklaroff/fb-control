{
  "name": "fb control",
  "manifest_version": 2,
  "version": "0.2",
  "description": "blocks the facebook newsfeed",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["jquery-1.9.1.min.js", "background.js"]
  },
  "content_scripts": [{
    "js": ["jquery-1.9.1.min.js", "fb_control.js"],
    "matches": ["http://www.facebook.com/*", "https://www.facebook.com/*"]
  }]
}
